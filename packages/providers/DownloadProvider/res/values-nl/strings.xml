    <string msgid="1672759909065511233" name="permdesc_seeAllExternal">"Hiermee kan de app alle downloads op de SD-kaart zien, ongeacht welke app ze heeft gedownload."</string>
    <string msgid="3069534308882047412" name="permlab_downloadCacheNonPurgeable">"Ruimte in de downloadcache reserveren"</string>
    <string msgid="2408760720334570420" name="permdesc_downloadCacheNonPurgeable">"Hiermee kan de app bestanden downloaden naar de downloadcache die niet automatisch kunnen worden verwijderd wanneer downloadbeheer meer ruimte nodig heeft."</string>
    <string msgid="8837971946078327262" name="permlab_downloadWithoutNotification">"bestanden downloaden zonder melding"</string>
    <string msgid="8483135034298639727" name="permdesc_downloadWithoutNotification">"Hiermee kan de app bestanden downloaden via downloadbeheer zonder dat de gebruiker een melding krijgt."</string>
