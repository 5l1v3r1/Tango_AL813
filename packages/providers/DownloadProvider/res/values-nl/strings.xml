    <string msgid="3658948994665187911" name="app_label">"Downloadbeheer"</string>
    <string msgid="7779544811202855500" name="permlab_downloadManager">"Downloadbeheer weergeven."</string>
    <string msgid="4237406545998908947" name="permdesc_downloadManager">"Hiermee heeft de app toegang tot downloadbeheer en kan de app via downloadbeheer bestanden downloaden. Schadelijke apps kunnen dit gebruiken om downloads te verstoren en toegang te krijgen tot persoonlijke gegevens."</string>
    <string msgid="7103642833308809655" name="permlab_downloadManagerAdvanced">"Geavanceerde functies van de downloadbeheerder."</string>
    <string msgid="2659546004160962761" name="permdesc_downloadManagerAdvanced">"Hiermee heeft de app toegang tot de geavanceerde functies van downloadbeheer. Schadelijke apps kunnen dit gebruiken om downloads te verstoren en toegang te krijgen tot persoonlijke gegevens."</string>
    <string msgid="945913803765675685" name="permlab_downloadCompletedIntent">"Downloadmeldingen verzenden."</string>
    <string msgid="2094706189855699533" name="permdesc_downloadCompletedIntent">"Hiermee kan de app meldingen verzenden over voltooide downloads. Schadelijke apps kunnen dit gebruiken om andere apps die bestanden downloaden in verwarring te brengen."</string>
    <string msgid="4084575448409212628" name="permlab_seeAllExternal" product="nosdcard">"Alle downloads naar USB-opslag bekijken"</string>
